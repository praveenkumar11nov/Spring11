<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/jsp">
	
	<decorator name="registeruser" page="login.jsp">
		<pattern>/forgotpwd</pattern>
	</decorator>
	
	<decorator name="loginpage" page="login.jsp">
		<pattern>/</pattern>
